{
  "name": "spotify-mini",
  "version": "0.1.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "type": "module",
  "author": {
    "email": "rocktimthedev@gmail.com",
    "name": "Rocktim Saikia",
    "url": "https://rocktimcodes.site"
  },
  "funding": "https://buymeacoffee.com/rocktimcodes",
  "repository": "rocktimsaikia/spotify-mini",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsup index.ts --dts --format esm",
    "pretest": "npm run build",
    "test": "ava --timeout 10s",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "ava": "^4.0.1",
    "dotenv": "^16.0.0",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "spotify-types": "^1.0.5",
    "ts-node": "^10.5.0",
    "tsup": "^5.11.13",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "isomorphic-unfetch": "^3.1.0"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "np": {
    "contents": "dist"
  },
  "keywords": [
    "spotify",
    "spotify-api",
    "spotify-typescript",
    "spotify-current-song",
    "spotify-last-played-song"
  ]
}
